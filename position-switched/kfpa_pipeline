#!/bin/bash

if [[ "$PATH" != */home/sandboxes/kfpa_pipeline* ]]
then
    PATH=/home/sandboxes/kfpa_pipeline:"$PATH"
fi

if [ -z "$PYTHONPATH" ]
then
    PYTHONPATH=/home/sandboxes/kfpa_pipeline
else
    lclpythonpath="$PYTHONPATH"
    if [[ $lclpythonpath != */home/sandboxes/kfpa_pipeline* ]]
    then
        PYTHONPATH=/home/sandboxes/kfpa_pipeline:"$PYTHONPATH"
    fi
fi

kfpa_pipeline.py $*
